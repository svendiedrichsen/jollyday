<?xml version="1.0" encoding="UTF-8"?>
<tns:Configuration hierarchy="uk" description="United Kingdom"
	xmlns:tns="http://jollyday.sourceforge.net/Jollyday" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jollyday.sourceforge.net/Jollyday ../Holiday.xsd">
	<tns:Holiday>
		<tns:Fixed month="DECEMBER" day="26"
			descriptionPropertiesKey="BOXING_DAY" />
		<tns:MovingCondition to="NEXT" weekday="TUESDAY">
			<tns:when>SATURDAY</tns:when>
			<tns:when>SUNDAY</tns:when>
		</tns:MovingCondition>
	</tns:Holiday>
	<tns:Holiday>
		<tns:Fixed month="DECEMBER" day="25"
			descriptionPropertiesKey="CHRISTMAS" />
		<tns:MovingCondition to="NEXT" weekday="MONDAY">
			<tns:when>SATURDAY</tns:when>
			<tns:when>SUNDAY</tns:when>
		</tns:MovingCondition>
	</tns:Holiday>
	<tns:Holiday>
		<tns:FixedWeekdayInMonth which="FIRST"
			weekday="MONDAY" month="MAY" descriptionPropertiesKey="BANK_HOLIDAY" />
	</tns:Holiday>
	<tns:Holiday>
		<tns:FixedWeekdayInMonth which="LAST" weekday="MONDAY"
			month="MAY" descriptionPropertiesKey="BANK_HOLIDAY" />
	</tns:Holiday>
	<tns:Holiday>
		<tns:ChristianHoliday type="EASTER" />
	</tns:Holiday>
	<tns:Holiday>
		<tns:ChristianHoliday type="GOOD_FRIDAY" />
	</tns:Holiday>
	<tns:SubConfigurations hierarchy="al"
		description="Alderney">
		<tns:Holiday>
			<tns:Fixed month="JANUARY" day="1"
				descriptionPropertiesKey="NEW_YEAR" />
		</tns:Holiday>
		<tns:Holiday>
			<tns:Fixed month="DECEMBER" day="15"
				descriptionPropertiesKey="HOMECOMING" />
		</tns:Holiday>
		<tns:Holiday>
			<tns:ChristianHoliday type="EASTER_MONDAY" />
		</tns:Holiday>
	</tns:SubConfigurations>
	<tns:SubConfigurations hierarchy="en"
		description="England">
		<tns:Holiday>
			<tns:Fixed month="DECEMBER" day="15"
				descriptionPropertiesKey="HOMECOMING" />
		</tns:Holiday>
		<tns:Holiday>
			<tns:Fixed month="JANUARY" day="1"
				descriptionPropertiesKey="NEW_YEAR" />
			<tns:MovingCondition to="NEXT" weekday="MONDAY">
				<tns:when>SATURDAY</tns:when>
				<tns:when>SUNDAY</tns:when>
			</tns:MovingCondition>
		</tns:Holiday>
		<tns:Holiday>
			<tns:FixedWeekdayInMonth which="LAST"
				weekday="MONDAY" month="AUGUST" descriptionPropertiesKey="BANK_HOLIDAY" />
		</tns:Holiday>
		<tns:Holiday>
			<tns:ChristianHoliday type="EASTER_MONDAY" />
		</tns:Holiday>
	</tns:SubConfigurations>
	<tns:SubConfigurations hierarchy="gu"
		description="Guernsey">
		<tns:Holiday>
			<tns:Fixed month="JANUARY" day="1"
				descriptionPropertiesKey="NEW_YEAR" />
		</tns:Holiday>
		<tns:Holiday>
			<tns:Fixed month="MAY" day="9" descriptionPropertiesKey="LIBERATION" />
		</tns:Holiday>
		<tns:Holiday>
			<tns:ChristianHoliday type="EASTER_MONDAY" />
		</tns:Holiday>
	</tns:SubConfigurations>
	<tns:SubConfigurations hierarchy="im"
		description="Isle of Man">
		<tns:Holiday>
			<tns:Fixed month="JANUARY" day="1"
				descriptionPropertiesKey="NEW_YEAR" />
		</tns:Holiday>
		<tns:Holiday>
			<tns:Fixed month="JULY" day="5" descriptionPropertiesKey="TYNWALD" />
		</tns:Holiday>
		<tns:Holiday>
			<tns:FixedWeekdayInMonth which="SECOND"
				weekday="FRIDAY" month="JUNE" descriptionPropertiesKey="TOURIST_TROPHY" />
		</tns:Holiday>
		<tns:Holiday>
			<tns:ChristianHoliday type="EASTER_MONDAY" />
		</tns:Holiday>
	</tns:SubConfigurations>
	<tns:SubConfigurations hierarchy="je"
		description="Jersey">
		<tns:Holiday>
			<tns:Fixed month="JANUARY" day="1"
				descriptionPropertiesKey="NEW_YEAR" />
		</tns:Holiday>
		<tns:Holiday>
			<tns:Fixed month="MAY" day="9" descriptionPropertiesKey="LIBERATION" />
		</tns:Holiday>
		<tns:Holiday>
			<tns:ChristianHoliday type="EASTER_MONDAY" />
		</tns:Holiday>
	</tns:SubConfigurations>
	<tns:SubConfigurations hierarchy="ni"
		description="Northern Ireland">
		<tns:Holiday>
			<tns:Fixed month="JANUARY" day="1"
				descriptionPropertiesKey="NEW_YEAR" />
		</tns:Holiday>
		<tns:Holiday>
			<tns:Fixed month="MARCH" day="17" descriptionPropertiesKey="ST_PATRICK" />
			<tns:MovingCondition to="NEXT" weekday="MONDAY">
				<tns:when>SATURDAY</tns:when>
				<tns:when>SUNDAY</tns:when>
			</tns:MovingCondition>
		</tns:Holiday>
		<tns:Holiday>
			<tns:Fixed month="JULY" day="12" descriptionPropertiesKey="BATTLE_BOYNE" />
			<tns:MovingCondition to="NEXT" weekday="MONDAY">
				<tns:when>SATURDAY</tns:when>
				<tns:when>SUNDAY</tns:when>
			</tns:MovingCondition>
		</tns:Holiday>
		<tns:Holiday>
			<tns:ChristianHoliday type="EASTER_MONDAY" />
		</tns:Holiday>
	</tns:SubConfigurations>
	<tns:SubConfigurations hierarchy="sc"
		description="Scotland">
		<tns:Holiday>
			<tns:Fixed month="JANUARY" day="1"
				descriptionPropertiesKey="NEW_YEAR" />
			<tns:MovingCondition to="NEXT" weekday="TUESDAY">
				<tns:when>SATURDAY</tns:when>
				<tns:when>SUNDAY</tns:when>
			</tns:MovingCondition>
		</tns:Holiday>
		<tns:Holiday>
			<tns:Fixed month="JANUARY" day="2"
				descriptionPropertiesKey="NEW_YEAR" />
			<tns:MovingCondition to="NEXT" weekday="MONDAY">
				<tns:when>SATURDAY</tns:when>
				<tns:when>SUNDAY</tns:when>
			</tns:MovingCondition>
		</tns:Holiday>
		<tns:Holiday>
			<tns:FixedWeekdayInMonth which="FIRST"
				weekday="MONDAY" month="AUGUST" descriptionPropertiesKey="BANK_HOLIDAY" />
		</tns:Holiday>
	</tns:SubConfigurations>
	<tns:SubConfigurations hierarchy="wa"
		description="Wales">
		<tns:Holiday>
			<tns:Fixed month="JANUARY" day="1"
				descriptionPropertiesKey="NEW_YEAR" />
			<tns:MovingCondition to="NEXT" weekday="MONDAY">
				<tns:when>SATURDAY</tns:when>
				<tns:when>SUNDAY</tns:when>
			</tns:MovingCondition>
		</tns:Holiday>
		<tns:Holiday>
			<tns:FixedWeekdayInMonth which="FIRST"
				weekday="MONDAY" month="AUGUST" descriptionPropertiesKey="BANK_HOLIDAY" />
		</tns:Holiday>
		<tns:Holiday>
			<tns:ChristianHoliday type="EASTER_MONDAY" />
		</tns:Holiday>
	</tns:SubConfigurations>
</tns:Configuration>
