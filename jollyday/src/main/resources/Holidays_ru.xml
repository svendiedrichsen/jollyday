		</tns:FixedMoving>
		<tns:FixedMoving month="MARCH" day="8">
			<tns:condition substitute="SATURDAY" with="NEXT" weekday="MONDAY"/>
			<tns:condition substitute="SUNDAY" with="NEXT" weekday="MONDAY"/>
		</tns:FixedMoving>
		<tns:FixedMoving month="MAY" day="1">
			<tns:condition substitute="SATURDAY" with="NEXT" weekday="MONDAY"/>
			<tns:condition substitute="SUNDAY" with="NEXT" weekday="MONDAY"/>
		</tns:FixedMoving>
		<tns:FixedMoving month="MAY" day="9">
			<tns:condition substitute="SATURDAY" with="NEXT" weekday="MONDAY"/>
			<tns:condition substitute="SUNDAY" with="NEXT" weekday="MONDAY"/>
		</tns:FixedMoving>
		<tns:FixedMoving month="JUNE" day="12" validFrom="1994">
			<tns:condition substitute="SATURDAY" with="NEXT" weekday="MONDAY"/>
			<tns:condition substitute="SUNDAY" with="NEXT" weekday="MONDAY"/>
		</tns:FixedMoving>
		<tns:FixedMoving month="NOVEMBER" day="4" validFrom="2005">
			<tns:condition substitute="SATURDAY" with="NEXT" weekday="MONDAY"/>
			<tns:condition substitute="SUNDAY" with="NEXT" weekday="MONDAY"/>
		</tns:FixedMoving>
	</tns:Holidays>
</tns:Configuration>
