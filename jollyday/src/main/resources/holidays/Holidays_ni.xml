<?xml version="1.0" encoding="UTF-8"?>
<tns:Configuration hierarchy="ni" description="Nicaragua"
	xmlns:tns="http://www.example.org/Holiday" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/Holiday Holiday.xsd ">
	<tns:Holidays>
		<tns:Fixed month="JANUARY" day="1" />
		<tns:Fixed month="FEBRUARY" day="1" />
		<tns:Fixed month="MAY" day="1" />
		<tns:Fixed month="MAY" day="27" />
		<tns:Fixed month="JULY" day="19" />
		<tns:Fixed month="SEPTEMBER" day="14" />
		<tns:Fixed month="SEPTEMBER" day="15" />
		<tns:Fixed month="OCTOBER" day="12" />
		<tns:Fixed month="DECEMBER" day="8" />
		<tns:Fixed month="DECEMBER" day="12" />
		<tns:Fixed month="DECEMBER" day="25" />
		<tns:Fixed month="DECEMBER" day="31" />
