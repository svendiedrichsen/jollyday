	</tns:Holidays>
	<tns:SubConfigurations hierarchy="bo" description="Boaco">
		<tns:Holidays>
			<tns:Fixed month="JULY" day="25"/>
		</tns:Holidays>
	</tns:SubConfigurations>
	<tns:SubConfigurations hierarchy="md" description="Madriz">
		<tns:Holidays>
			<tns:Fixed month="JULY" day="25"/>
		</tns:Holidays>
	</tns:SubConfigurations>
	<tns:SubConfigurations hierarchy="mn" description="Managua">
		<tns:Holidays>
			<tns:Fixed month="JULY" day="25"/>
			<tns:Fixed month="AUGUST" day="1"/>
		</tns:Holidays>
	</tns:SubConfigurations>
</tns:Configuration>
